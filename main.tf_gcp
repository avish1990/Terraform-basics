provider "google" {
  credentials = ""
  project     = "avinashgcp"
   region      = "asia-south1"
}


resource "google_compute_instance" "avish-tf" {
  machine_type = "n1-standard-1"
  name         = "avish-tf"
  zone         = "asia-south1-a"

  boot_disk {
    initialize_params {
      image = "debian-cloud/debian-8"
    }
  }
  network_interface {
    network = "default"

    access_config {
      // Ephemeral IP
    }
  }
}
